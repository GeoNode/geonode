<ivy-module version="2.0">
    <info organisation="org.ringojs" module="ringojs"/>
    <dependencies>
        <!-- rhino -->
        <dependency org="org.mozilla" name="rhino" rev="1.7R3"/>
        <!-- jetty -->
        <dependency org="org.eclipse.jetty" name="jetty-client" rev="7.5.+"/>
        <dependency org="org.eclipse.jetty" name="jetty-server" rev="7.5.+"/>
        <dependency org="org.eclipse.jetty" name="jetty-servlet" rev="7.5.+"/>
        <dependency org="org.eclipse.jetty" name="jetty-websocket" rev="7.5.+"/>
        <dependency org="org.eclipse.jetty" name="jetty-xml" rev="7.5.+"/>
        <dependency org="javax.servlet" name="servlet-api" rev="2.5"/>
        <!-- logging -->
        <dependency org="org.slf4j" name="slf4j-api" rev="1.6.+"/>
        <dependency org="org.slf4j" name="slf4j-log4j12" rev="1.6.+">
            <exclude org="log4j"/>
        </dependency>
        <dependency org="log4j" name="log4j" rev="1.2.14"/>
        <!-- jnr-posix -->
        <dependency org="org.jruby.extras" name="jaffl" rev="0.5.+"/>
        <dependency org="org.jruby.ext.posix" name="jnr-posix" rev="1.1.+"/>
        <!-- junit -->
        <dependency org="junit" name="junit" rev="3.8.+"/>
    </dependencies>
</ivy-module>
