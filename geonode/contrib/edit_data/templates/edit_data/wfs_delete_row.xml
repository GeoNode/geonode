<wfs:Transaction service="WFS" version="1.0.0"
  xmlns:ogc="http://www.opengis.net/ogc"
  xmlns:wfs="http://www.opengis.net/wfs">
  <wfs:Delete typeName="{{ workspace }}:{{layer_name}}">
    <ogc:Filter>
      <ogc:FeatureId fid="{{ feature_id }}"/>
    </ogc:Filter>
  </wfs:Delete>
</wfs:Transaction>
