					<mxGeometry as="geometry" relative="1"/>
				</mxCell>
			</Connector>
		</add>
		<add as="embalse">
			<Symbol name="Embalse" label="Damming" externalData="false">
				<mxCell vertex="1" style="embalse" dbreference="EMBALSE">      
					<mxGeometry as="geometry" width="60" height="92"/>
				</mxCell>
			</Symbol>
		</add>
		<add as="bombeo">
