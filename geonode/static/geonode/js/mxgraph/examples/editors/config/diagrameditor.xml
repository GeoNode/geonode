		</add>
		<add as="bocatomaflotante">
			<Symbol name="Bocatoma Flotante" label="Floating Intake" externalData="false">
				<mxCell vertex="1" style="bocatomaflotante" dbreference="BOCATOMAFLOTANTE">      
					<mxGeometry as="geometry" width="60" height="92"/>
				</mxCell>
			</Symbol>
		</add>
		<add as="desarenador">
			<Symbol name="Desarenador" label="Desander" externalData="false">
				<mxCell vertex="1" style="desarenador" dbreference="DESARENADOR">      
					<mxGeometry as="geometry" width="60" height="92"/>
